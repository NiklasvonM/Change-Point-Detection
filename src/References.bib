@article{[0]BUCCHIA2017344,
title = {\href{https://www.sciencedirect.com/science/article/pii/S0047259X17300179}{Change-point detection and bootstrap for Hilbert space valued random fields}},
journal = {Journal of Multivariate Analysis},
volume = {155},
pages = {344-368},
year = {2017},
issn = {0047-259X},
doi = {https://doi.org/10.1016/j.jmva.2017.01.007},
url = {https://www.sciencedirect.com/science/article/pii/S0047259X17300179},
author = {Béatrice Bucchia and Martin Wendler},
keywords = {Change-point detection, Dependent wild bootstrap, FCLT for Hilbert space valued r.v., Random fields},
abstract = {The problem of testing for the presence of epidemic changes in random fields is investigated. In order to be able to deal with general changes in the marginal distribution, a Cramér–von Mises type test is introduced which is based on Hilbert space theory. A functional central limit theorem for {$\rho$}-mixing Hilbert space valued random fields is proven. In order to avoid the estimation of the long-run variance and obtain critical values, Shao's dependent wild bootstrap method is adapted to this context. For this, a joint functional central limit theorem for the original and the bootstrap sample is shown. Finally, the theoretic results are supplemented by a short simulation study.}
}

